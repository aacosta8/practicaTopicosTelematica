<% cache publication do%>
  <div class="w3-container w3-red">
    <h4><%= link_to publication.name , publication, class:"be-white"%></h4>
    <img src="<%=publication.photo.url%>"  style="width:100%">
  </div>

  <div class="w3-container w3-black">
    <h5>Tomada en: <%=publication.place%></h5>
    <p>Descripcion: <%=publication.description%></p>
  </div><br>
<%end%>
